{
    "update_url": "https://clients2.google.com/service/update2/crx",
    "name": "Gepetto",
    "description": "TODO",
    "browser_action": {
        "default_icon": {
            "16": "/icons/icon16.png",
            "48": "/icons/icon48.png",
            "96": "/icons/icon96.png"
        },
        "default_title": "Gepetto"
    },
    "permissions": [
        "webRequest"
    ],
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [ "<all_urls>" ],
            "js": ["inject.js"],
            "css" : ["inject.css"],
			"run_at": "document_end"
        }
    ],
    "optional_host_permissions": [
        "*://*/*"
    ],
    
    "icons": {
        "16": "/icons/icon16.png",
        "48": "/icons/icon48.png",
        "96": "/icons/icon96.png"
    },
    "content_security_policy" : {
        "extension_pages" : "script-src 'self';"
    },
	
    "version": "0.0.3"
}