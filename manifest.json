{
    "update_url": "https://clients2.google.com/service/update2/crx",
    "name": "Gepetto",
    "description": "extract the claims in the text and find out what would be some good questions to ask about it",
    "browser_action": {
        "default_icon": {
            
            "48": "/icons/icon48.png",
            "96": "/icons/icon96.png",
            "128": "/icons/icon128.png"
        },
        "default_title": "Gepetto"
    },
    "permissions": [
        "webRequest"
    ],
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [ "<all_urls>" ],
            "js": ["inject.js"],
            "css" : ["inject.css"],
			"run_at": "document_end"
        }
    ],
    "icons": {
        "48": "/icons/icon48.png",
        "96": "/icons/icon96.png",
        "128": "/icons/icon128.png"
    },
    "content_security_policy" : {
        "extension_pages" : "script-src 'self';"
    },
    "web_accessible_resources": [{
        "matches": [ "<all_urls>" ],
        "resources": ["inject.panel.css", "icon.png", "dq.png"]
    }],
    "version": "0.0.5"
}